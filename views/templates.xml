<?xml version="1.0" encoding="utf-8"?>
<odoo>

  <!-- Homepage Template -->
  <template id="homepage_template" name="Landing Page">
    <t t-call="website.layout">
      <div id="wrap" class="oe_structure">

        <!-- 1. HERO SECTION -->
        <section class="position-relative d-flex align-items-center"
          style="height: 80vh; background-image: url('/om_hr_portal/static/img/landing_hero.png'); background-size: cover; background-position: center;">
          <div class="position-absolute top-0 start-0 w-100 h-100"
            style="background-color: rgba(0, 0, 0, 0.5);"></div>
          <div class="container text-center text-white position-relative">
            <h1 class="display-3 fw-bolder mb-3">Design That Inspires. Quality That Lasts.</h1>
            <p class="lead col-lg-8 mx-auto">Discover handcrafted furniture that transforms your
              house into a home. Explore our collections and find the perfect piece for your space.</p>
            <div class="d-grid gap-2 d-sm-flex justify-content-sm-center mt-4">
              <a href="/products" class="btn btn-primary btn-lg px-4 gap-3">Explore Collections</a>
              <a href="/about-us" class="btn btn-outline-light btn-lg px-4">Our Story</a>
            </div>
          </div>
        </section>

        <!-- 2. CORE SERVICES/VALUES SECTION -->
        <section class="py-5 bg-light">
          <div class="container">
            <div class="row g-4 text-center">
              <div class="col-lg-4">
                <div
                  class="feature-icon d-inline-flex align-items-center justify-content-center text-bg-primary bg-gradient fs-3 mb-3 rounded-circle"
                  style="width: 4rem; height: 4rem;">
                  <i class="fa fa-star" />
                </div>
                <h3 class="fs-4 fw-bold">Uncompromising Quality</h3>
                <p class="text-muted">From premium materials to meticulous craftsmanship, every
                  piece is built to the highest standard.</p>
              </div>
              <div class="col-lg-4">
                <div
                  class="feature-icon d-inline-flex align-items-center justify-content-center text-bg-primary bg-gradient fs-3 mb-3 rounded-circle"
                  style="width: 4rem; height: 4rem;">
                  <i class="fa fa-pencil-square-o" />
                </div>
                <h3 class="fs-4 fw-bold">Bespoke Design</h3>
                <p class="text-muted">Work with our designers to create custom furniture that is
                  uniquely yours.</p>
              </div>
              <div class="col-lg-4">
                <div
                  class="feature-icon d-inline-flex align-items-center justify-content-center text-bg-primary bg-gradient fs-3 mb-3 rounded-circle"
                  style="width: 4rem; height: 4rem;">
                  <i class="fa fa-leaf" />
                </div>
                <h3 class="fs-4 fw-bold">Sustainable Practice</h3>
                <p class="text-muted">We are committed to eco-friendly processes and responsibly
                  sourced materials.</p>
              </div>
            </div>
          </div>
        </section>

        <!-- 3. FEATURED PRODUCTS SECTION -->
        <section class="py-5">
          <div class="container">
            <h2 class="text-center display-6 fw-bold mb-5">Featured Products</h2>
            <div class="row">
              <t t-if="not featured_products">
                <div class="col-12 text-center text-muted">Featured products will be displayed here.</div>
              </t>
              <t t-foreach="featured_products" t-as="product">
                <div class="col-md-4 mb-4">
                  <div class="card h-100 shadow-sm border-0">
                    <div class="card-img-top" style="height: 250px; overflow: hidden;">
                      <a t-att-href="product.website_url or '#'">
                        <img t-attf-src="/web/image/product.template/{{product.id}}/image_512"
                          class="img img-fluid" alt="{{product.name}}"
                          style="object-fit: cover; width: 100%; height: 100%;" />
                      </a>
                    </div>
                    <div class="card-body">
                      <h5 class="card-title">
                        <a t-att-href="product.website_url or '#'" class="text-decoration-none"
                          t-esc="product.name" />
                      </h5>
                      <p class="fw-bold" t-field="product.list_price"
                        t-options="{'widget': 'monetary', 'display_currency': website.currency_id}" />
                    </div>
                  </div>
                </div>
              </t>
            </div>
            <div class="text-center mt-4">
              <a href="/products" class="btn btn-outline-primary">View All Products</a>
            </div>
          </div>
        </section>

        <!-- 4. TESTIMONIAL SECTION -->
        <section class="py-5 bg-light">
          <div class="container">
            <h2 class="text-center display-6 fw-bold mb-5">What Our Customers Say</h2>
            <div class="row g-4">
              <div class="col-md-6">
                <div class="card h-100 border-0 shadow-sm">
                  <div class="card-body text-center">
                    <img src="/om_hr_portal/static/img/testimonial_avatar_1.png" alt="Client 1"
                      class="rounded-circle mb-3"
                      style="width: 80px; height: 80px; object-fit: cover;" />
                    <p class="fst-italic">"The quality of the dining table we purchased is simply
                      outstanding. It has become the centerpiece of our home. The entire process,
                      from consultation to delivery, was seamless."</p>
                    <h5 class="fw-bold mt-3 mb-0">Jessica Miller</h5>
                    <small class="text-muted">Homeowner</small>
                  </div>
                </div>
              </div>
              <div class="col-md-6">
                <div class="card h-100 border-0 shadow-sm">
                  <div class="card-body text-center">
                    <img src="/om_hr_portal/static/img/testimonial_avatar_2.png" alt="Client 2"
                      class="rounded-circle mb-3"
                      style="width: 80px; height: 80px; object-fit: cover;" />
                    <p class="fst-italic">"We worked with the design team for a custom bookshelf,
                      and they brought our vision to life perfectly. The craftsmanship is
                      impeccable. Highly recommended!"</p>
                    <h5 class="fw-bold mt-3 mb-0">David Chen</h5>
                    <small class="text-muted">Interior Designer</small>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </section>

        <!-- 5. FINAL CALL TO ACTION (CTA) -->
        <section class="container py-5">
          <div class="p-5 text-center bg-light rounded-3">
            <h2 class="fw-bold">Ready to Elevate Your Space?</h2>
            <p class="col-lg-8 mx-auto fs-5 text-muted">
              Whether you're looking for a single piece or need help designing an entire room, our
              team is here to help.
            </p>
            <a href="/contactus" class="btn btn-primary btn-lg mt-3">Start a Conversation</a>
          </div>
        </section>

      </div>
    </t>
  </template>

  <!-- About Us Page Template -->
  <template id="about_us_template" name="About Us">
    <t t-call="website.layout">
      <div id="wrap" class="oe_structure">

        <!-- 1. Hero Section -->
        <section
          class="position-relative text-center text-white"
          style="background-image: url('/om_hr_portal/static/img/about_hero.png'); background-size: cover; background-position: center;"
        >
          <div class="position-absolute top-0 start-0 w-100 h-100"
            style="background-color: rgba(0, 0, 0, 0.5);"></div>
          <div class="container position-relative py-5">
            <h1 class="display-3 fw-bold my-5">Crafting Spaces, Inspiring Lives</h1>
            <p class="lead col-lg-8 mx-auto">
              Our mission is to merge timeless design with sustainable practices, creating furniture
              that brings comfort and style to every home.
            </p>
          </div>
        </section>

        <!-- 2. Our Story Section -->
        <div class="container my-5">
          <div class="row align-items-center g-5">
            <div class="col-lg-6">
              <h2 class="display-5 fw-bold mb-3">Our Journey</h2>
              <p class="text-muted">
                Founded in 2010 from a small workshop, our passion for quality
                craftsmanship has driven us to become a trusted name in the furniture industry. We
                started with a simple idea: to build beautiful, durable furniture that people love.
              </p>
              <p class="text-muted">
                Today, our team of skilled artisans and designers continue that
                tradition, blending classic techniques with modern innovation to create pieces that
                are not only aesthetically pleasing but also built to last for generations.
              </p>
            </div>
            <div class="col-lg-6">
              <img src="/om_hr_portal/static/img/workshop_image.png"
                class="img-fluid rounded shadow-lg" alt="Our Workshop" />
            </div>
          </div>
        </div>

        <!-- 3. Our Core Values Section -->
        <div class="bg-light py-5">
          <div class="container">
            <h2 class="text-center fw-bold mb-5">Our Core Values</h2>
            <div class="row g-4 text-center">
              <div class="col-lg-4">
                <div
                  class="feature-icon d-inline-flex align-items-center justify-content-center text-bg-primary bg-gradient fs-3 mb-3 rounded-circle"
                  style="width: 4rem; height: 4rem;"
                >
                  <i class="fa fa-star" />
                </div>
                <h3 class="fs-4">Uncompromising Quality</h3>
                <p>We use only the finest materials and time-honored techniques to ensure every
                  piece meets the highest standards of quality.</p>
              </div>
              <div class="col-lg-4">
                <div
                  class="feature-icon d-inline-flex align-items-center justify-content-center text-bg-primary bg-gradient fs-3 mb-3 rounded-circle"
                  style="width: 4rem; height: 4rem;"
                >
                  <i class="fa fa-leaf" />
                </div>
                <h3 class="fs-4">Sustainability</h3>
                <p>We are committed to eco-friendly practices, from sourcing sustainable wood to
                  minimizing waste in our production process.</p>
              </div>
              <div class="col-lg-4">
                <div
                  class="feature-icon d-inline-flex align-items-center justify-content-center text-bg-primary bg-gradient fs-3 mb-3 rounded-circle"
                  style="width: 4rem; height: 4rem;"
                >
                  <i class="fa fa-lightbulb-o" />
                </div>
                <h3 class="fs-4">Customer-Centric Innovation</h3>
                <p>We constantly listen to our customers' needs to innovate and design furniture
                  that is both beautiful and functional for modern living.</p>
              </div>
            </div>
          </div>
        </div>

        <!-- 4. Call to Action Section -->
        <div class="container py-5 text-center">
          <h2 class="fw-bold">Ready to Explore?</h2>
          <p class="lead text-muted col-lg-6 mx-auto">Discover our collection or see if there's a
            place for you on our team.</p>
          <a href="/products" class="btn btn-primary btn-lg mt-3">View Our Products</a>
          <a href="/jobs" class="btn btn-outline-secondary btn-lg mt-3">Join Our Team</a>
        </div>

      </div>
    </t>
  </template>

  <!-- Services Page Template -->
  <template id="services_template" name="Services">
    <t t-call="website.layout">
      <div id="wrap" class="oe_structure">

        <!-- 1. Hero Section -->
        <section class="container text-center py-5">
          <h1 class="display-5 fw-bold">From Concept to Completion</h1>
          <p class="lead text-muted col-lg-8 mx-auto">
            We offer a comprehensive range of services to bring your vision to life.
            Our team is dedicated to providing exceptional quality and a seamless experience from
            start to finish.
          </p>
        </section>

        <!-- 2. Services Grid -->
        <div class="bg-light py-5">
          <div class="container">
            <div class="row g-4 row-cols-1 row-cols-md-3">
              <!-- Service 1: Custom Design -->
              <div class="col">
                <div class="card h-100 text-center shadow-sm border-0">
                  <div class="card-body">
                    <div
                      class="feature-icon d-inline-flex align-items-center justify-content-center text-bg-primary bg-gradient fs-3 mb-3 rounded-circle"
                      style="width: 4rem; height: 4rem;"
                    >
                      <i class="fa fa-pencil-square-o" />
                    </div>
                    <h3 class="card-title fs-4">Custom Furniture Design</h3>
                    <p class="card-text">
                      Work with our expert designers to create bespoke pieces
                      that perfectly fit your vision and space.
                      From initial sketch to final product, we guide you through every step.
                    </p>
                  </div>
                </div>
              </div>
              <!-- Service 2: Consultation -->
              <div class="col">
                <div class="card h-100 text-center shadow-sm border-0">
                  <div class="card-body">
                    <div
                      class="feature-icon d-inline-flex align-items-center justify-content-center text-bg-primary bg-gradient fs-3 mb-3 rounded-circle"
                      style="width: 4rem; height: 4rem;"
                    >
                      <i class="fa fa-comments-o" />
                    </div>
                    <h3 class="card-title fs-4">Interior Design Consultation</h3>
                    <p class="card-text">
                      Our experienced designers help you transform your home or office.
                      We provide consultation on choosing the right pieces, colors, and
                      layouts to create a cohesive space.
                    </p>
                  </div>
                </div>
              </div>
              <!-- Service 3: Delivery -->
              <div class="col">
                <div class="card h-100 text-center shadow-sm border-0">
                  <div class="card-body">
                    <div
                      class="feature-icon d-inline-flex align-items-center justify-content-center text-bg-primary bg-gradient fs-3 mb-3 rounded-circle"
                      style="width: 4rem; height: 4rem;"
                    >
                      <i class="fa fa-truck" />
                    </div>
                    <h3 class="card-title fs-4">Delivery &amp; Installation</h3>
                    <p class="card-text">
                      We offer professional, white-glove delivery and installation services to
                      ensure your new furniture arrives safely and is set up perfectly in your
                      space.
                    </p>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>

        <!-- 3. Call to Action (CTA) -->
        <section class="container text-center py-5">
          <h2 class="fw-bold">Let's Build Something Amazing Together</h2>
          <p class="lead text-muted col-lg-6 mx-auto">
            Have an idea? We'd love to hear about it.
            Contact us today for a free consultation.
          </p>
          <a href="/contactus" class="btn btn-primary btn-lg mt-3">Get in Touch</a>
        </section>
      </div>
    </t>
  </template>

  <!-- TERMS AND CONDITIONS PAGE TEMPLATE -->
  <template id="terms_and_conditions_template" name="Terms and Conditions">
    <t t-call="website.layout">
      <div id="wrap" class="oe_structure">
        <section class="container py-5">

          <!-- 1. Page Header -->
          <div class="text-center mb-5">
            <h1 class="display-5 fw-bold">Terms and Conditions</h1>
          </div>

          <div class="row justify-content-center">
            <div class="col-lg-8">
              <p class="lead mb-4">
                Please read these Terms and Conditions ("Terms") carefully before using our website
                and purchasing our products or services. By accessing or using our services, you
                agree to be bound by these Terms.
              </p>

              <!-- 2. Table of Contents -->
              <div class="card mb-5">
                <div class="card-header">Table of Contents</div>
                <ul class="list-group list-group-flush">
                  <li class="list-group-item">
                    <a href="#section-1" class="text-decoration-none">1. General Terms</a>
                  </li>
                  <li class="list-group-item">
                    <a href="#section-2" class="text-decoration-none">2. Products and Orders</a>
                  </li>
                  <li class="list-group-item">
                    <a href="#section-3" class="text-decoration-none">3. Custom Design Services</a>
                  </li>
                  <li class="list-group-item">
                    <a href="#section-4" class="text-decoration-none">4. Pricing and Payment</a>
                  </li>
                  <li class="list-group-item">
                    <a href="#section-5" class="text-decoration-none">5. Shipping and Delivery</a>
                  </li>
                  <li class="list-group-item">
                    <a href="#section-6" class="text-decoration-none">6. Returns and Refunds</a>
                  </li>
                  <li class="list-group-item">
                    <a href="#section-7" class="text-decoration-none">7. Warranty</a>
                  </li>
                  <li class="list-group-item">
                    <a href="#section-8" class="text-decoration-none">8. Limitation of Liability</a>
                  </li>
                  <li class="list-group-item">
                    <a href="#section-9" class="text-decoration-none">9. Governing Law</a>
                  </li>
                </ul>
              </div>

              <!-- 3. Detailed Content -->
              <div id="section-1" class="mb-5">
                <h3 class="fw-bold border-bottom pb-2 mb-3">1. General Terms</h3>
                <p>This website is operated by VNDT. Throughout the site, the terms
                  “we”, “us” and “our” refer to VNDT. By visiting our site and/or
                  purchasing something from us, you engage in our “Service” and agree to be bound by
                  the following terms and conditions.</p>
              </div>

              <div id="section-2" class="mb-5">
                <h3 class="fw-bold border-bottom pb-2 mb-3">2. Products and Orders</h3>
                <p>Product information, including images and descriptions, is provided for
                  informational purposes. While we strive for accuracy, variations in color,
                  texture, and grain are natural characteristics of wood and other materials and are
                  not considered defects. All orders are subject to acceptance and availability.</p>
              </div>

              <div id="section-3" class="mb-5">
                <h3 class="fw-bold border-bottom pb-2 mb-3">3. Custom Design Services</h3>
                <p>For custom design services, a non-refundable design fee is required to begin the
                  process. All custom designs, drawings, and specifications remain the intellectual
                  property of VNDT until the final product is paid for in full.
                  Timelines for custom projects are estimates and may vary.</p>
              </div>

              <div id="section-4" class="mb-5">
                <h3 class="fw-bold border-bottom pb-2 mb-3">4. Pricing and Payment</h3>
                <p>All prices are listed in USD and are subject to change
                  without notice. We accept payments via Visa, MasterCard, Bank Transfer.
                  For custom orders, a 50% deposit is required to
                  commence production, with the remaining balance due prior to shipping.</p>
              </div>

              <div id="section-5" class="mb-5">
                <h3 class="fw-bold border-bottom pb-2 mb-3">5. Shipping and Delivery</h3>
                <p>Shipping costs are calculated at checkout and vary based on destination and order
                  size. We offer standard and white-glove delivery options. Delivery times
                  are estimates and we are not liable for delays caused by third-party carriers or
                  unforeseen circumstances.</p>
              </div>

              <div id="section-6" class="mb-5">
                <h3 class="fw-bold border-bottom pb-2 mb-3">6. Returns and Refunds</h3>
                <p> Standard, in-stock items may be returned within 14 days of receipt for a refund,
                  minus a restocking fee. Items must be in original, unused condition. <strong>Custom-made
                  and special-order items are non-returnable and non-refundable.</strong>
                </p>
              </div>

              <div id="section-7" class="mb-5">
                <h3 class="fw-bold border-bottom pb-2 mb-3">7. Warranty</h3>
                <p>We offer a 1-year limited warranty on our furniture against manufacturing
                  defects. This warranty does not cover normal wear and tear, damage from misuse, or
                  variations in natural materials. For warranty claims, please contact us with your
                  proof of purchase.</p>
              </div>

              <div id="section-8" class="mb-5">
                <h3 class="fw-bold border-bottom pb-2 mb-3">8. Limitation of Liability</h3>
                <p>In no event shall VNDT, our directors, or employees be liable for
                  any injury, loss, or claim arising from your use of any of our products or
                  services.</p>
              </div>

              <div id="section-9" class="mb-5">
                <h3 class="fw-bold border-bottom pb-2 mb-3">9. Governing Law</h3>
                <p>These Terms shall be governed by and construed in accordance with the laws of
                  Vietnam, without regard to its conflict of law provisions.</p>
              </div>

              <p class="text-center fst-italic">If you have any questions about these Terms, please <a
                  href="/contactus">contact us</a>.</p>
            </div>
          </div>
        </section>
      </div>
    </t>
  </template>

  <!-- Jobs Listing Page Template -->
  <template id="job_list_template" name="Jobs">
    <t t-call="website.layout">
      <div id="wrap">
        <div class="container py-5">
          <div class="text-center mb-5">
            <h1 class="display-5 fw-bold">Open Positions</h1>
            <p class="lead text-muted">Join our growing team and help us build the future.</p>
          </div>

          <div class="row justify-content-center">
            <div class="col-lg-8">
              <t t-if="not jobs">
                <div class="alert alert-info">
                  There are currently no open positions. Please check
                  back later!
                </div>
              </t>

              <t t-foreach="jobs" t-as="job">
                <!-- Job Card -->
                <div class="card mb-3 shadow-sm">
                  <div class="card-body">
                    <div class="d-flex justify-content-between align-items-center">
                      <h4 class="card-title mb-0">
                        <a t-att-href="job.website_url or f'/jobs/detail/{job.id}'"
                          class="text-decoration-none">
                          <t t-esc="job.name" />
                        </a>
                      </h4>
                      <a t-att-href="job.website_url or f'/jobs/detail/{job.id}'"
                        class="btn btn-primary btn-sm stretched-link">
                        View Details
                      </a>
                    </div>
                    <div class="text-muted mt-2">
                      <span class="me-3" t-if="job.department_id">
                        <i class="fa fa-building fa-fw me-1" />
                        <t t-esc="job.department_id.name" />
                      </span>
                      <span class="me-3" t-if="job.address_id">
                        <i class="fa fa-map-marker fa-fw me-1" />
                        <t t-esc="job.address_id.city or 'Remote'" />
                      </span>
                      <span class="me-3" t-if="job.job_type">
                        <i class="fa fa-briefcase fa-fw me-1" />
                        <t
                          t-esc="dict(job.fields_get(['job_type'])['job_type']['selection'])[job.job_type]" />
                      </span>
                    </div>
                  </div>
                </div>
              </t>
            </div>
          </div>
        </div>
      </div>
    </t>
  </template>

  <!-- Job Detail Page Template -->
  <template id="job_detail_template" name="Job Detail">
    <t t-call="website.layout">
      <div id="wrap">
        <div class="container py-5">
          <!-- Job Description -->
          <h1 t-field="job.name" />
          <div class="mt-4" id="job_description">
            <t t-raw="job.description" />
          </div>

          <hr class="my-5" />

          <!-- Application Form -->
          <h2>Apply for this position</h2>
          <form action="/jobs/apply" method="POST" enctype="multipart/form-data">
            <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()" />
            <input type="hidden" name="job_id" t-att-value="job.id" />
            <div class="mb-3">
              <label for="applicant_name" class="form-label">Full Name</label>
              <input type="text" class="form-control" name="applicant_name" required="1" />
            </div>
            <div class="mb-3">
              <label for="applicant_email" class="form-label">Email Address</label>
              <input type="email" class="form-control" name="applicant_email" required="1" />
            </div>
            <div class="mb-3">
              <label for="applicant_phone" class="form-label">Phone Number</label>
              <input type="tel" class="form-control" name="applicant_phone" />
            </div>
            <div class="mb-3">
              <label for="cv_file" class="form-label">Upload your CV</label>
              <input type="file" class="form-control" name="cv_file" required="1" />
            </div>
            <button type="submit" class="btn btn-primary">Submit Application</button>
          </form>
        </div>
      </div>
    </t>
  </template>

  <!-- Thank You Page Template -->
  <template id="job_thank_you_template" name="Thank You">
    <t t-call="website.layout">
      <div id="wrap">
        <div class="container py-5 text-center">
          <h1 class="display-4">
            Application Submitted Successfully!
          </h1>
          <p class="lead">
            Thank you for applying for this job position. We will process your CV and get in touch
            within the next few days.
          </p>
          <a href="/jobs" class="btn btn-primary mt-3">
            Back to Job Listings
          </a>
        </div>
      </div>
    </t>
  </template>

  <!-- Products Page Template -->
  <template id="products_template" name="Products">
    <t t-call="website.layout">
      <div id="wrap" class="oe_structure">
        <div class="container py-5">
          <h1 class="text-center mb-5">Our Furniture Collection</h1>
          <div class="row">
            <t t-if="not products">
              <div class="col-12">
                <div class="alert alert-info text-center">
                  There are currently no products available to display.
                </div>
              </div>
            </t>
            <t t-foreach="products" t-as="product">
              <div class="col-md-4 mb-4">
                <div class="card h-100 shadow-sm">

                  <div class="card-img-top" style="height: 250px; overflow: hidden;">
                    <a t-att-href="product.website_url or '#'">
                      <img t-attf-src="/web/image/product.template/{{product.id}}/image_1920"
                        class="img img-fluid"
                        alt="{{product.name}}"
                        style="object-fit: cover; width: 100%; height: 100%;" />
                    </a>
                  </div>

                  <div class="card-body d-flex flex-column">
                    <h5 class="card-title">
                      <a t-att-href="product.website_url or '#'" class="text-decoration-none">
                        <t t-esc="product.name" />
                      </a>
                    </h5>
                    <div class="mt-auto">
                      <span class="fw-bold fs-5" t-field="product.list_price"
                        t-options="{'widget': 'monetary', 'display_currency': website.currency_id}" />
                    </div>
                  </div>
                </div>
              </div>
            </t>
          </div>
        </div>
      </div>
    </t>
  </template>

  <!-- Error Page Template -->
  <template id="job_error_template" name="Application Error">
    <t t-call="website.layout">
      <div id="wrap">
        <div class="container py-5 text-center">
          <h1 class="display-4 text-danger">An Error Occurred</h1>
          <p class="lead">
            We're sorry, but something went wrong while submitting your application.
          </p>
          <p>
            Please try again later or contact our support team.
          </p>
          <div t-if="error" class="alert alert-danger mt-3" t-esc="error" />
          <a href="/jobs" class="btn btn-secondary mt-3">
            Back to Job Listings
          </a>
        </div>
      </div>
    </t>
  </template>

</odoo>
